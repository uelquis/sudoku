<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" stylesheets="@style/index.css" xmlns="http://javafx.com/javafx/21.0.7" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.uelquis.sudoku.ui.controllers.MainMenuController">

    <VBox fx:id="vbox" prefHeight="240.0" prefWidth="600.0">
        <Label fx:id="gameTitle" text="Sudoku" />
        <Button onAction="#startGame" styleClass="start_game_btn" text="Play" />
      <Button layoutX="272.0" layoutY="136.0" onAction="#configGame" styleClass="config_btn" text="Config" />
    </VBox>

</AnchorPane>
