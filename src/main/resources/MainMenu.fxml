<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="dev.uelquis.sudoku.ui.controllers.MainMenuController"
            prefHeight="600.0" prefWidth="600.0"
            stylesheets="style/index.css">

    <VBox fx:id="vbox">
        <Label text="Sudoku" fx:id="gameTitle" />
        <Button text="Play" onAction="#startGame"/>
    </VBox>

</AnchorPane>
